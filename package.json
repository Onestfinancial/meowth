{
  "name": "meowth",
  "version": "1.0.0",
  "description": "Microservice in charge of the customer Reward balance management and operations",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "serve": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "create:migration": "npx typeorm-ts-node-commonjs migration:generate src/migration/UpdateEntities -d src/config/data-source.ts",
    "run:migration": "npx typeorm-ts-node-commonjs migration:run -d src/config/data-source.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "module-alias": "^2.2.3",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "serverless-dotenv-plugin": "^3.9.2",
    "serverless-http": "^3.2.0",
    "serverless-plugin-log-retention": "^2.0.0",
    "serverless-ssm-publish": "^1.7.0",
    "@types/node": "^20.14.8",
    "@types/express": "^4.17.21",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@routes": "dist/routes",
    "@controllers": "dist/controllers",
    "@middlewares": "dist/middlewares",
    "@models": "dist/models",
    "@services": "dist/services",
    "@utils": "dist/utils"
  }
}
